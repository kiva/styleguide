	<div id="faq-hkw-section" class="faq-section">
		<div class="faq-container">
			<h2 class="faq-title white">Frequently asked questions</h2>
			<section class="accordion-container">
				{{#faqs}}
					<div class="ac-title ac-container white {{id}}" data-kv-accordion aria-controls="{{id}}" aria-expanded="false">
						<p kvtrackevent="How Kiva Works|click-FAQ" class="question-wrap">
							{{question}}
						</p>
							<span class="ac-dropdown-icon">
								<svg class="icon icon-small-chevron">
									<use xlink:href="#icon-small-chevron"></use>
								</svg>
							</span>

					</div>
						<article class="ac-body" id="{{id}}" aria-hidden="true">
								<span>
									<p class='small-text white'>{{{answer}}}</p>
								</span>
						</article>

				{{/faqs}}
				<div class="help-center-link white">
					<a kvtrackevent="How Kiva Works|click-Learn More" href="http://www.kiva.org/help">Learn more in the help center</a>
				</div>
			</section>
		</div>
		<div class="white text-center">
			<p class="start-lending-text featured">Start lending!</p>
			<a class="choose-borrower" href="https://www.kiva.org/categories">
				<div class="choose-borrower-frame button" >
					<p kvtrackevent="How Kiva Works|click-Choose a borrower" class="choose-borrower-text">Choose a borrower</p>
				</div>
			</a>
		</div>
	</div>
