<div class="{{name}}-button-box triple-state-gender-buttons">
    <input type="radio" class="ts-women" id="filter-{{name}}-{{women.id}}" name="{{name}}" value="{{women.value}}" />
    <label class="ts-label interact" for="filter-{{name}}-{{women.id}}">{{women.label}}</label>

    <input type="radio" class="ts-men" id="filter-{{name}}-{{men.id}}" name="{{name}}" value="{{men.value}}" />
    <label class="ts-label interact" for="filter-{{name}}-{{men.id}}">{{men.label}}</label>

    <input type="radio" class="ts-nonbinary" id="filter-{{name}}-{{nonbinary.id}}" name="{{name}}"
        value="{{nonbinary.value}}" />
    <label class="ts-label interact" for="filter-{{name}}-{{nonbinary.id}}">{{nonbinary.label}}</label>

    <input type="radio" id="filter-{{name}}-none" name="{{name}}" value="none" />
    <label for="filter-{{name}}-none" class="ts-floater ts-label">&nbsp;</label>

    <div style="clear: both;"></div>
</div>
