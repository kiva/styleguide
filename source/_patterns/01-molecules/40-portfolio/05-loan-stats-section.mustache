<div class="loan-stats-section">
    <h2 class="row">
        <span class="section-title">{{sectionTitle}}</span>
        <span class="lent-count">{{numLentTo}}/{{totalNum}}</span>
    </h2>
    <div class="row sub-title">
        <div class="small-6 large-6 columns">
            You've lent to
        </div>
        <div class="small-6 large-6 columns">
            You haven't lent to
        </div>
    </div>
    <div class="row">
        <div class="small-6 large-6 columns">
            <ul class="small-block-grid-1 medium-block-grid-2">
                {{#sectionListLentTo}}
                    <li>
                        <img src="{{iconURL}}">
                        <a href="{{labelLink}}">{{label}}</a>
                    </li>
                {{/sectionListLentTo}}
            </ul>
        </div>
        <div class="small-6 large-6 columns">
            <ul class="small-block-grid-1 medium-block-grid-2">
                {{#sectionListNotLentTo}}
                    <li>
                        <img src="{{iconURL}}">
                        <a href="{{labelLink}}">{{label}}</a>
                    </li>
                {{/sectionListNotLentTo}}
            </ul>
        </div>
    </div>
</div>
