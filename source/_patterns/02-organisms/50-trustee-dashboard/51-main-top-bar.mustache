<div class="trustee-dashboard-main">
	<div class="row">
		<div class="columns large-12">
			<div class="impact trustee-name">{{trustee.title}}</div>
		</div>
	</div>

	<div class="row">
		<div class="columns large-4 small-6">
			{{#trustee.imageSmall}}
				<a href="#"><img alt="Image for {{trustee.title}}" src="{{trustee.imageSmall}}"></a>
			{{/trustee.imageSmall}}
		</div>
		<div class="columns large-4 small-6">
			{{#trustee-stats.borrower-count-statistic}}
				{{> 01-molecules/06-components/10-statistic-display}}
			{{/trustee-stats.borrower-count-statistic}}
		</div>
		<div class="columns large-4 small-12">
			Invite Borrowers<br/>
			{{> 01-molecules/10-lightbox/14-trustee-invite-link-lightbox}}
		</div>
	</div>
</div>