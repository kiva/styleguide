<h3 class="filter show-for-medium-up">Partners</h3>

<h3 id="partner-accordion-selector" data-kv-accordion class="ac-container show-for-small-only" aria-controls="partnersFilter" aria-expanded="false">
    Partners
	<span class="ac-dropdown-icon">
		<svg class="icon icon-large-chevron" style="margin-right: 12px">
            <use xlink:href="#icon-large-chevron"></use>
        </svg>
	</span>
</h3>

<select name="partner[]" id="partnersFilter" multiple="multiple" aria-hidden="false">
    {{#partner-regions}}
        <optgroup label="{{label}}">
            {{#partners}}
                <option value="{{id}}">{{name}}</option>
            {{/partners}}
        </optgroup>
    {{/partner-regions}}
</select>